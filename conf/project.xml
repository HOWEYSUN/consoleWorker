<?xml version="1.0"?>
<bean name="tjManager" workerModuleObj="consoleRobot.webWorker.tjManager" workerModuleName="TjManagementWorker" >
    <page name="initUrl" url="https://passport.tujia.com/PortalSite/LoginPage/">
        <location by="xpath" value="//*[@id='nc_1_n1z']" waitUtil="True" wailTime="10">
            <ation by="click_and_hold"/>
            <ation by="perform" timeSleep="0.1"/>
            <ation by="move_by_offset" value="xoffset=50,yoffset=0" />
            <ation by="perform" timeSleep="0.1"/>
            <ation by="move_by_offset" value="xoffset=260,yoffset=0"/>
            <ation by="perform" timeSleep="0.1"/>
            <ation by="release"/>
            <ation by="perform" timeSleep="0.1"/>
        </location>
        <location by="xpath" value="//*[@id='app']/section/section[1]/section[3]/section[1]/div[2]/div[1]/div[1]/input"
                  callback="setUserName" />
        <location by="xpath" value="//*[@id='app']/section/section[1]/section[3]/section[1]/div[2]/div[1]/div[2]/input"
                  callback="setPwd" />
        <location by="xpath" value="//*[@id='app']/section/section[1]/section[3]/section[1]/div[2]/button">
            <ation by="click"/>
        </location>
    </page>
    <page name="mainPage" url="location">
        <location updated="yes">5</location>
    </page>
    <page name="orderList" url="location">
        <location updated="yes">5</location>
    </page>
</bean>